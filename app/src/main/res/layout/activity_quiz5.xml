<?xml version="1.0" encoding="utf-8"?>
<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:padding="8dp"
    android:layout_height="match_parent">
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".Quiz5Activity">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_q9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/q9"
        android:textColor="@color/colorAccent"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent" />
<androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/q1_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:padding="8dp"
    android:drawableRight="@drawable/ic_check"
            android:text="string resources"
            android:textSize="16dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/q1_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="activities in app"
        android:padding="8dp"
        android:drawableRight="@drawable/ic_check"

            android:textSize="16dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/q1_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
           android:layout_marginTop="10dp"
            android:text="images resources"
        android:padding="8dp"
        android:drawableRight="@drawable/ic_check"
            android:textSize="16dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/q1_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
        android:padding="8dp"
        android:drawableRight="@drawable/ic_check"
        android:text="permissions"
            android:textSize="16dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_q10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/q10"
        android:textColor="@color/colorAccent"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/q2_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:padding="8dp"
            android:drawableRight="@drawable/ic_check"
            android:text="Physical Android Device"
            android:textSize="16dp" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/q2_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:padding="8dp"
            android:drawableRight="@drawable/ic_check"
            android:text="AVD Manager"
            android:textSize="16dp" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/q2_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="SDK Manager"
            android:padding="8dp"
            android:drawableRight="@drawable/ic_check"
            android:textSize="16dp" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/q2_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:padding="8dp"
            android:drawableRight="@drawable/ic_check"
            android:text="Theme Editor"
            android:textSize="16dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_next1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/bg_start_btn"
        android:onClick="nextScreen5"
        android:padding="8dp"
        android:layout_marginTop="20dp"
        android:text="next"
        android:drawableRight="@drawable/ic_next"
        android:textColor="@android:color/white"
        android:textSize="18dp" />
</LinearLayout>
</ScrollView>
